    "@rollup/plugin-node-resolve": "15.3.1",
    "@sanity/eslint-config-studio": "4.0.0",
    "@types/jest": "29.5.14",
    "@types/node": "^22.0.0",
    "eslint": "8.57.1",
    "jest": "29.7.0",
    "prettier": "3.7.3",
    "regenerator-runtime": "0.14.1",
    "rollup": "4.53.3",
    "semantic-release": "24.2.9",
    "typescript": "5.9.3"
  }
